{
  "name": "py-bridge",
  "version": "1.0.0",
  "description": "Simple way to call existing python functions and get their return value in nodejs environment",
  "main": "./dist/ts/exec.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:ts": "tsc",
    "build:py": "cpy \"src/py/**/*\" dist/py",
    "build": "npm run clean && npm run build:ts && npm run build:py",
    "postinstall": "node ./dist/scripts/postinstall.js"
  },
  "files": [
    "dist",
    "requirements.txt"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@types/express": "^5.0.3",
    "cpy-cli": "^6.0.0",
    "express": "^5.1.0",
    "rimraf": "^6.0.1"
  }
}
